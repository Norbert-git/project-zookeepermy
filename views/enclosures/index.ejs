<body>
    <h1>Enclosures</h1>
    <section class="main">
        <div class="enclosures-main">
            <% enclosures.forEach(enclosure => { %>
            <div class="enclosure">
                <a href="/enclosures/<%= enclosure.id %>"><%= enclosure.name %></a><br>
                Occupancy: <%= enclosure.animals.length %>
                <!-- <a href="/enclosures/<%= enclosure.id %>"><img src="<%= enclosure.img %>" alt="<%= enclosure.name %>"></a><br>
                Occupancy: <%= enclosure.animals.length %> -->
            </div>
        </div>
        <% }) %>
    </section>
</body>
