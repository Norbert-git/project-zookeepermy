<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="/CSS/style.css">
    <title>Document</title>
</head>
<body>
    <%- include('../partials/header.ejs') %> 
    <h1>Enclosures</h1>
    <section class="main">
        <ul>
            <% enclosures.forEach(enclosure => { %>
            <li>
                <a href="/enclosures/<%= enclosure.id %>"><%= enclosure.name %></a><br>
                Occupancy: <%= enclosure.animals.length %>
            </li><br>
            <% }) %>
        </ul><br>
        <a href="/enclosures/new">Add Enclosure</a>
    </section>
</body>
</html>